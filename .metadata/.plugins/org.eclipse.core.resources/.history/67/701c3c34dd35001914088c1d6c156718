package pattern;
import java.util.Scanner;
public class Pattern3 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.err.println("Enter the range.");
		int n=sc.nextInt();
		for(int i=0;i<n;i++)
		{
			for (int j = 0; j <n ; j--) 
			{
				System.out.println("*"+" ");
			}
			System.out.println();
			sc.close();
		}
		
	}

}
