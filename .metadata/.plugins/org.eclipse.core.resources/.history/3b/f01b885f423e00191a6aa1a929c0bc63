		System.out.println("Enter that element which you want to find the index.");
		int n=sc.nextInt();
		int res=indexOf(n);
	}
	static int indexOf(int x)
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter how many elements you have to store.");
		int n=sc.nextInt();
		int ar[]=new int[n];
		System.out.println("Plz enter "+n+" elements.");
		for (int i = 0; i <n; i++) 
		{
			ar[i]=sc.nextInt();
		}
		System.out.println("Entered elements are :");
		for (int i = 0; i < n; i++) 
		{
			System.out.print(ar[i]+" ");
		}
		System.out.println();
	     return -1;
	}
